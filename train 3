using System;
class Building
{
    public int floors;
    public int area;
    public int occupants;
    public int AreaPP()
    {
        return area / occupants;
    }
}
class BuildingDemo
{
    static void Main()
    {
        Building house = new Building();
        Building office = house;
        

        house.occupants = 4;
        house.area = 250;
        house.floors = 2;

        Console.WriteLine($"House has: \n - {house.floors} floors.\n - {house.occupants} occupants.\n - {house.area} m. area\n {house.AreaPP()} m. per person");
        


        office.occupants = 25;
        office.area = 300;
        office.floors = 1;

        Console.WriteLine($"Office has: \n - {office.floors} floors.\n - {office.occupants} occupants.\n - {office.area} m. area\n - {office.AreaPP()} m. per person");
        



    }
}
